
body,td,th {
	color: #333333;
	font-family: Times New Roman, Times, serif;
	font-size: 12px;
}
body {
	text-align:center;
}
a:link {
	color: #333;
	text-decoration: none;
}
a:visited {
	text-decoration: none;
	color: #333;
}
a:hover {
	text-decoration: underline;
	color: #000;
}
a:active {
	text-decoration: none;
	color: #333;
}
#Main_frame{
	width:900px;
	text-align:left;
	padding:10px;
	margin:auto;
}
#options{
	background-image:url(images/block.png);
	width:200px;
	height:100px;
	text-align:center;
	vertical-align:middle;
	line-height:100px;
	margin:auto;
	display:block;
	float:left;
}
input
{
	border:1px;
	border-style:dashed;
	font-size:12px;
	background-color:#FFFFFF;
	text-align:center;
	width:80px;
}
#input_title{
	border:0px;
	font-size:12px;
	background-color:#CCC;
	text-align:center;

}
#plist td{
	padding:3px;
	background-color:#FFF;
}